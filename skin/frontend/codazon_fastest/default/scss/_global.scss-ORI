body{font:$f_default;color:$main_text;background:$page_background}
body a{color:$main_alink_text;text-decoration: none;  vertical-align: middle;}
a:hover{text-decoration: none; vertical-align: middle;}
a:hover, a:focus{color:$main_hover_text}
.wrapper{font:$f_default;color:$main_text;background-color:$page_background}
.pages{background:$page_background}
.space-base{margin-bottom:$cdz_space_area ;}
.space-small-base{margin-bottom:$cdz_space_small_area;}

.page-title h1,
.page-title h2
{
  text-transform: uppercase;
  font: $f_h3;
  font-weight: 600;
  color: $main_page_text_color;
  padding-bottom: 3px;
  margin-bottom: 15px;
  text-transform: uppercase;
}
.cdz-dropdown-content{
	background:$menu_drop_background;
	border-color:$menu_drop_border;
}


.breadcrumbs ul li{
	color:$main_page_text_color;
}

.cdz-best-seller-wra .nav-tabs .nav-tab-item .switch:hover {
    color: $main_hover_text;
    text-decoration: none;
  }
  .cdz-best-seller-wra .nav-tabs .nav-tab-item .switch:active {
    color: $main_hover_text;
    text-decoration: none;
  }
  .cdz-best-seller-wra .nav-tabs .nav-tab-item:not(.disabled) .switch:focus,
  .cdz-best-seller-wra .nav-tabs .nav-tab-item:not(.disabled) .switch:hover {
    background: transparent;
  }

  
/* ============================================ *
 * Content
 * ============================================ */

/*three block*/
/*Background parallax*/
.bg-wrapper-ads{
	background-image: url(../images/bg-parallax.jpg);
	background-position: center top;
	background-repeat: no-repeat;
	overflow: hidden;
	.bg-parallax{
		background-size: cover !important;
		-webkit-background-size: cover;
		-moz-background-size: cover;
		-o-background-size: cover;
		padding: 5rem 0;
	}
}
.groupbanner-text{
	.groupinfo{
		.groupinfo-inner{
			.groupinfo-desc{
				color:$main_hover_text_box;
				.text-link{
					:hover{ text-decoration:underline;color:$main_hover_text_box}
					:after{
						@include abs-iconfont('\f105', 14px);
					}
				}
				.action.primary{
					color: $btn_productitem_text;
					border-color: $btn_productitem_border;
					background: $btn_productitem_background;
					:hover{
						color: $btn_primary_text;
						border-color: $btn_primary_border;
						background: $btn_primary_background;
					}
				}
			}
		
		}
	}
}

/* ============================================ *
 * Codazon_Footer style 1
 * ============================================ */

.footer-top-ads {
	color:$main_text;
	
	.ftop-ads-small {
		color: $main_text;
	}
	.block.block-subscribe {
		.actions{
			.button {
				color: $btn_default_text;
				background-color: $btn_default_background;
				border-color: $btn_default_border;
				font:$f_h4;
			}
			.button:hover {
				background:$btn_primary_background;
				color:$btn_primary_text;
				border-color:$btn_primary_border;
			}
		}
	}
}

.block.block-subscribe{
	.block-content {
		input {
			border-color:$footer_border;
		}
	}
}

.page-footer {
	background: $footer_background;
	color: $footer_text;
	.top-footer-wrapper {
		background-color:$footer_top_background;
	}
	.footer-bottom {
		background:$footer_bottom_background;
	}
	a {
		color: $footer_text;
	}
	a:hover {
		color: $footer_hover_text;
	}
	p.h5 {
		color:$footer_text;
		font:$f_h5;
	}
	.item_tag {
		li{
			a{
				color: $footer_text;
				background-color: $footer_bottom_background;
				border-color: $footer_border;
			}
			a:hover {
				color: $btn_default_text;
				background-color: $btn_default_background;
				border-color: $btn_default_border;
			}
		}
	}
	.footer-social {
		a {
			border: 1px solid;
			border-color: $footer_border;
			&:hover{
				color: $footer_bottom_background;
				background-color: $footer_text;
				border-color: $footer_border;
			}
		}
	}
	
	.bottom-footer-wrapper {
		background: $footer_bottom_background;
		border-top-color: $footer_border;
	}
	
	.copyright {
		font: $f_h5;
	}
	.footer-bottom-link {
		li {
			a {
			  font: $f_h5;
			}
		}
	}
}
.cdz-footer-style-03{
	.top-footer-wrapper{
	}
	.bottom-footer-wrapper{
		address{
			font:$f_note;
		}
	}
	.footer-social{
		a{
			&:hover{
				background:none;
				border-color:none;
				&:before{
					border-color:$footer_hover_text;
				}
				span{
					color:$footer_hover_text
				}
			}
			&:before {
				border-color:$footer_text;
			}
		}
	}
}
/* ============================================ *
 * End Codazon_Footer Style 1
 * ============================================ */
 
