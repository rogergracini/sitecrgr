}
 .menu-vertical{
	.cdz-nav-wrap{
		border:1px solid $menu_vertical_border;
		li.vertical-level1{
			> a.menu-link{
				color:$menu_vertical_text;
				background-color:$menu_vertical_background;
			}
			&.view-all{
				a{
					color:$menu_vertical_text;
					background-color:$menu_vertical_background;
				}
			}
			&:hover{
				> a.menu-link{
					background:$menu_vertical_hover_background;
					color:$menu_vertical_hover_text	
				} 
				.cdz-sub-cat{
				}
			}
			
			.cdz-sub-cat{
				background:$menu_vertical_drop_background;
				border-color:$menu_vertical_drop_border;
				a{
					color:$menu_vertical_drop_text;
					&:hover,&:active{
						color:$menu_vertical_drop_hover_text;
					}
				}
				li.level4{
					border-bottom-color:$menu_vertical_border;
					color:$menu_vertical_text;
				}
			}
		}
		.parent{
			&:after{
				color:$menu_vertical_text;
			}
			&:hover{
				&:after{
					color:$menu_vertical_hover_text;
				}		
			}
		}
		.cdz-sub-cat .parent{
			&:after{
				color:$menu_vertical_drop_text;
			}
			&:hover{
				&:after{
					color:$menu_vertical_drop_hover_text;
				}		
			}
		}
		.view-all{
			a{
				color:$main_hover_text;
			}
		}
	}
}
