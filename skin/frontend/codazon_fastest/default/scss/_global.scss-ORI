/*Default Input and Validate input --- */
input.input-text{
	font:$f_h6;
	width:100%;
	height:40px!important;
	color:$input_text;
	border: 1px solid $input_border;
}
textarea.input-text{
	font:$f_h6;
	width:100%;
	color:$input_text;
	border: 1px solid $input_border;
	max-width:100%
}
.validate-select{
	font:$f_h6;
	width:100%;
	height:40px!important;
	color:$input_text;
	border: 1px solid $input_border;
	max-width:inherit
}

.input-text:focus {
	border: 1px solid $input_border_focus;
	outline-color:$input_border_focus
}

.input-text.validation-failed { border-color: #fc7070;}
.validate-select.validation-failed { border-color: #fc7070;}
.input-text.validation-failed:focus { outline-color: #ef9384;}
/*---Default Input and Validate input */


