			}
		}
		&:not(.disabled){
			&.active .switch,
			.switch:focus,
			.switch:hover{
				background: transparent;
				border-bottom-color:$main_border_dark;
				color:$main_tab_hover_text;
			}
			.switch:active{
				color: $main_tab_hover_text;
				background: transparent;
				border-bottom-color:$main_title_border;
			}
		}
		&.active{
			.switch,.switch:focus,.switch:hover {
				color: $main_tab_hover_text;
				background: transparent;
				border-bottom-color:$main_title_border;
			}
		}
		h2{
			font: $f_h5;
		}
	}	
}
 /* ============================================ *
 * Codazon_Tab style
 * ============================================ */ 
/*product label*/
.cdz-product-lbs{
	.lb-item{
		background: $label_new_background;
		color: $label_product_text;
		&.lb-sale{
			background: $label_sale_background;
		}
	}
}
/*Box Style Home Sport*/
.box-style01{
	background: $main_block_background;
	.cdz-block-title{
		color: $main_title_text;
		border-color:$main_border;
		.lastcolor{
			color:$main_hover_text;
		}
	}
	.products-grid{
		border-left:1px solid $main_border;
		.product-item{
			.product-item-info{
				border:1px solid $main_border;
				border-left:none;
			}
		}
	}
	.products-list{
		.cdz-buttons-container button.btn-cart{
			font:$f_h6;
			&:before{display:none;}
		}
	}
	.nav.nav-tabs{		
		.nav-tab-item .switch{
			font:$f_h3;
		}
	}
	.owl-nav [class*='owl-'], 
	.owl-theme .owl-nav [class*=owl-]{
		border:1px solid $main_title_border;
		background: $main_block_background;
		color: $main_title_border;
		&:hover{
			border-color:$main_hover_text;
			color: $main_hover_text;
		}
	}
	&.cdz-wapper-blog-01 .cdz_wapper-blog-content{
		.owl-nav [class*='owl-'], 
		.owl-theme .owl-nav [class*=owl-]{
			border:1px solid $main_title_border;
			background: $main_block_background;
			color: $main_title_border;
			&:hover{
				border-color:$main_hover_text;
				color: $main_hover_text;
			}
		}
	}
}

/*button show01 Home Sport*/
.cdz-button-show01{
	.products-grid .product-item-info {
		.product-item-details{
		}
		&:hover{	
			background: $main_product_hover_background;
			.product-item-details{				
				background: $main_product_hover_background;
			}
		}
	}
}

/*button show01 Home Outdoor*/
@media (min-width: 992px){
.cdz-button-show02{
	.products-grid .product-item-info {
		.cdz-buttons-container{
			button.btn-cart{
				background: $btn_default_background;
				color: $btn_default_text;
				border-color: $btn_default_border;
				border-bottom-color: darken($btn_secondary_border,5%);
				&:hover{
					background: $btn_primary_background;
					color: $btn_primary_text;
					border-color: $btn_primary_border;
				}
			}
			.qs-button,
			a.action{
				background: $btn_secondary_background;
				color: $btn_secondary_text;
				border-color: $btn_secondary_border;
				border-bottom-color: darken($btn_secondary_border,5%);
				&:hover{
					background: $btn_primary_background;
					color: $btn_primary_text;
					border-color: $btn_primary_border;
				}
			}
		}
	}
}
}

.modal-content{border-radius: $border_radius;}

@media (max-width: 767px){
	.nav.nav-tabs.mobile-nav-tabs .nav-tab-item, 
	.mobile-nav-tabs .nav-tab-item{
		.switch{
			border:none;
			border-bottom-color: transparent;
			border-bottom-style: solid;
			border-bottom-width: 2px;
			&:hover,{
				border-bottom-color: $main_tab_border;
			}
		}
		&.active{
			.switch{
				border-bottom-color: $main_tab_border;
			}
		}
	}
}

/*outdoor*/
.line-style01{
	.cdz-block-title{
		border-color:$main_title_border;
		.b-title{
			font:$f_h3;
			background: $main_block_title_background;
