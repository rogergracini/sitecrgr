/*Search box area*/
.cdz-header-menu{
	background:$menu_background
}
.block-search {
	.label:before {
	}
	.label:hover:before {
		color: $main_text;
	}
	.label:active:before {
		color: $main_text;
	}
	.control {
		border-top: 1px solid $main_border;
	}

	input[type="text"] {
		color: $header_search_text;
		background-color: $header_search_background;
		font:$f_note;
	}
	.button:hover:before {
		color: $main_text;
	}
	.button.disabled,	
	.button:focus:before {
		color: $main_text;
	}
	.button:before {
		color: $header_search_text;
	}
	.cat-search-enable{
		border-color: darken($header_search_background,15%);
		color: $header_search_text;
		background-color:$header_search_background;
		.current-cat{
			border-color:darken($header_search_background,15%);
		}
		.button{
			background-color:$btn_default_background;
			&:before{color:$btn_default_text;}
			&:hover{
				background-color:$menu_background;
				&:before{color:$menu_text;}
			}
		}
	}
}
.block-search .button[disabled],
fieldset[disabled] .block-search .button {
	cursor: not-allowed;
	pointer-events: none;
	opacity: 0.5;
}

.block-search input ::-webkit-input-placeholder {
  color: $header_text;
}
.block-search input :-moz-placeholder {
  color: $header_text;
}
.block-search input ::-moz-placeholder {
  color: $header_text;
}
.block-search input :-ms-input-placeholder {
  color: $header_text;
}


/*----- Codazon Search box*/

.search-autocomplete {
	ul {
		li {
			border-top: 1px solid #e5e5e5;
