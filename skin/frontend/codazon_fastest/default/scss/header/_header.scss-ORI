	.actions{border-bottom:0px solid $main_border!important;padding:0px!important;}
}
/*Logo and Cart area*/
/*================ CdzCart Bottom ===================*/
.footer-cart {
    position: fixed;
    bottom: 0;
    width: 100%;
    z-index: 100;
    left: 0;
	&:before{
		content: '';
		position: absolute;
		width: 100%;
		top: -3px;
		left: 0;
		height: 3px;
		box-shadow: 0 2px 3px #e1e1e1;
	}
	.cart-trigger{
		position: absolute;
		background: #fc7070;
		color: #fff;
		top: -34px;
		left:0;
		font-size: 85%;
		text-transform: uppercase;
		padding: 10px 20px 7px;
		z-index: 200;
		cursor: pointer;
		text-decoration:none;
		&:after{
			@include font-awesome(\f106,16px);
			margin:0 5px;
			float:right;
			-moz-transition: all 500ms ease;
			-webkit-transition: all 500ms ease;
			-o-transition: all 500ms ease;
			transition: all 500ms ease;
			-moz-backface-visibility: hidden;
			-webkit-backface-visibility: hidden;
			backface-visibility: hidden;
			margin-top:-1px;
		}
	}
	&.active{
		.cart-trigger{
			&:after{
				-moz-transform: rotate(180deg);
				-o-transform: rotate(180deg);
				transform: rotate(180deg);
			}
		}
	}
	.cart-content{
		background: #fff;
		padding: 0;
		overflow: hidden;
		position: relative;
		min-height: 85px;
		box-shadow: 0px 0px 15px rgba(0,0,0,0.3);
		.cart-icon{
			width:0;
			height:0;
			@include abs-opacity(0);
			visibility:hidden;
		}
		.check-out{
			float:right;
			padding:20px;
			width:421px;
			position:absolute;
			top:0;
			right:0;
			border-left:1px solid #dfdfdf;
			.summary{
				float:left;
				width:190px;
				.cart-total-title{
					display:inline-block;
					width:100%;
					h4{float:left; margin:0; margin-right:10px;}
					.amount{float:right;margin:0;}
				}
				.subtotal{
					display:inline-block;
					width:100%;
					.sub-total-label{float:left; margin-right:10px;}
					.price{float:right;font-weight:700;}
				}
			}
			.actions{
				float:right;
 				height: 100px;
 				margin: -20px 0;
 				border-left: 1px solid #dfdfdf;
 				padding: 20px 0;
				padding-left: 20px;
				button.button{
					padding: 10px 20px;
					&:before{
						@include font-awesome(\f00c,16px);
						margin-right:10px;
					}
				}
			}
		}
		.cart-items{
			float:left;
			padding-left:20px;
			width: calc(100% - 420px);
			overflow-x: auto;
			
			.cart-items-inner{display:block;overflow-x:auto;}
 			.cart-products-list{float:left;    white-space: nowrap; font-size:0;}
			.product{
				position:relative;
				cursor: pointer;
				strong{
					position: absolute;
					top: -5px;
					right: -5px;
					z-index: 1;
					background: #fc7070;
					color: #fff;
					display: block;
					border-radius: 100%;
					width: 16px;
					height: 16px;
					text-align: center;
					font-size: 11px;
					font-weight:500;
				}
				&:before{
					@include font-awesome(\f14b,16px);
					position: absolute;
					bottom: 0;
					right: 5px;
					z-index: 1;
				}
			}
			.cart-item{
				position:relative;
				width: 80px;
				margin-right:10px;
				display: inline-block;
				font-size:13px;
				padding:10px 0;
				input.item-qty{
					height:23px;
					width:40px;
					text-align:center;
				}
				.actions{}
				.btn-remove{
					border:none;
					background:none;
					display:block;
					float:none;
					clear:both;
					font-size: 10px;
					width: auto;
					color:#f86532;
					line-height: 1.35;
					margin-top: 17px;
					text-decoration:underline;
					text-transform:uppercase;
					&:before{
						//@include font-awesome(\f1f8,18px);
					}
					&:after{
						display:none;
					}
				}
				.btn-edit{
					font-size:11px;
					float:right;
					padding:3px 5px 2px;
					&:before{
						display:none;
					}
				}
				.product-details{
					position: absolute;
					left: 100%;
					top:0;
					float: left;
					height: 100%;
					padding:5px 10px;
					background: #F4F4F4;
					width: 0;
					visibility: hidden;
					overflow: hidden;
					-webkit-opacity: 0;
					-moz-opacity: 0;
					opacity: 0;
					filter: alpha(opacity=0);
					-moz-transition: all 500ms ease;
					-webkit-transition: all 500ms ease;
					-o-transition: all 500ms ease;
					transition: all 500ms ease;
					-moz-transition: opacity .4s ease, transform .4s ease, visibility .4s linear 0s;
					-webkit-transition: opacity .4s ease, transform .4s ease, visibility .4s linear 0s;
					-o-transition: opacity .4s ease, transform .4s ease, visibility .4s linear 0s;
					transition: opacity .4s ease, transform .4s ease, visibility .4s linear 0s;
					-moz-backface-visibility: hidden;
					-webkit-backface-visibility: hidden;
					backface-visibility: hidden;
					-moz-transform: translateX(-30px);
					-webkit-transform: translateX(-30px);
					-o-transform: translateX(-30px);
					transform: translateX(-30px);
					z-index: 2;
					.product-hover-container{
						display: table;
						width: 100%;
						height: 100%;
						.product-hover-inner{
							display: table-cell;
							vertical-align: middle;
						}
					}
				}
				&.active .product-details{
					width: 100px;
					padding: 5px 10px;
					-webkit-opacity: 1;
					-moz-opacity: 1;
					opacity: 1;
					filter: alpha(opacity=100);
					-moz-transform: translateX(0);
					-webkit-transform: translateX(0);
					-o-transform: translateX(0);
					transform: translateX(0);
					visibility: inherit;
				}
			}
			.owl-nav [class*='owl-'], 
			.owl-theme .owl-nav [class*=owl-]{
				width:30px;
				height:30px;
				@include border-radius(0);
				&.owl-next{
					left:-35px;
					top:10px;
					right:auto;
					&:before{
						line-height: 21px;
						font-size: 25px;
					}
				}
				&.owl-prev{
					left: -35px;
					top: 45px;
					right:auto;
					&:before{
						line-height: 21px;
						font-size: 25px;
					}
				}
			}
		}
	}
}

/*Custom RTL*/
.cdz-rtl{
	.cdz-top-login.cdz-dropdown .cdz-dropdown-content,
	.block-cart.cdz-dropdown-content{
		left:0;
		right:auto;
	}
	.block-search{
		float:left;
		.button{float:left; left: 0; right: auto;}
	}
	.sticky-menu:not(.active){
		.cat-search-enable{
			.cat-input{
				float:right;
				.current-cat{
					border-left-style: solid;
					border-left-width: 2px;
					border-right-width: 0px;
				}
				.cdz-dropdown-content{
					left:auto;
					right:0;
				}
			}
			input[type="text"]{
				float:left;
				padding-left:40px;
				padding-right:10px;
			}
		}
	}
	.header .cdz-header-top{
		.cdz-dropdown{
			margin-left: 15px;
			margin-right: 0px;
		}
		.cdz-top-link{	
			float:left;
			.link{    
				margin-left: 0px;
				margin-right: 20px;
			}
			li a:before{
				margin-right: 0px;
				margin-left: 5px;
			}
		}
	}
}